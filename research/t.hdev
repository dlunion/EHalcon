<procedure name="p_do_2dbarcode">
<interface/>
<body>
<l>read_image (ImageECC200, 'datacode/ecc200/ecc200_disturbed_005')</l>
<l>crop_rectangle1 (ImageECC200, ImageECC200Part, 110, 30, 110 + 284, 30 + 284)</l>
