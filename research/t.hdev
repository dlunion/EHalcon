<l>disp_message (WindowHandle1, 'Micro QR', 'image', 240 + 284, 12, 'black', 'true')</l>
<l>disp_message (WindowHandle1, 'PDF 417', 'image', 240 + 284, 12 + 284, 'black', 'true')</l>
<c>* </c>
